<h1>Lista de Productos</h1>
<div class="contenedorCards">
   {{#each productos}}
   <div class="card">
      <p>{{this.title}}</p>
      <p>{{this.description}}</p>
      <p>{{this.price}}</p>
      <p>{{this.img}}</p>
      <p>{{this.code}}</p>
      <p>{{this.stock}}</p>
      <p>{{this.category}}</p>
   </div>
   {{/each}}
</div>
